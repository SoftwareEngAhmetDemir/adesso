{
    "setupFiles": ["<rootDir>/src/setupTests.tsx"],
    "testRegex": "/*.test.tsx$",
    "collectCoverage": true,
    "coverageReporters": ["lcov"],
    "coverageDirectory": "test-coverage",
    "coverageThreshold": {
     "global": {
     "branches": 80,
     "functions": 80,
     "lines": 80,
     "statements": 80
     }
    },
    "moduleDirectories": ["node_modules", "src"]
   }